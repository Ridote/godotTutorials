[sub_resource type="GDScript" id=1]
script/source = "extends PanelContainer

const OPTIONS_SCENE = \"res://Scenes/GUI/Options.tscn\"

onready var start = $\"CenterContainer/VBoxContainer/StartGame\"
onready var options = $\"CenterContainer/VBoxContainer/Options\"
onready var exit = $\"CenterContainer/VBoxContainer/Exit\"

func _ready():
	start.connect(\"pressed\", self, \"_on_start\")
	options.connect(\"pressed\", self, \"_on_options\")
	exit.connect(\"pressed\", self, \"_on_exit\")

func _on_start():
	if(get_tree().change_scene(OPTIONS_SCENE)):
		print(\"Error when trying to start the game\")

func _on_options():
	if(get_tree().change_scene(OPTIONS_SCENE)):
		print(\"Error when trying to go to options menu from the main menu\")

func _on_exit():
	get_tree().quit()"

